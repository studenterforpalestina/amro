generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model PressPost {
  id        Int      @id @default(autoincrement())
  slug      String   @unique
  title     String
  date      DateTime
  content   String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
